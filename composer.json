{
    "name": "sidneyallen/xero-php",
    "description": "php lib for Xero API generated by OpenAPI spec for oAuth2",
    "keywords": [
        "xero",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "http://developer.xero.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Xero API Team",
            "homepage": "https://developer.xero.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "league/oauth2-client": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "SidneyAllen\\XeroPHP\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SidneyAllen\\XeroPHP\\" : "test/" }
    }
}
